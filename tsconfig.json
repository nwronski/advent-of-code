{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "lib": [
      "es2020"
    ],
    "module": "commonjs",
    "target": "es2020",
    "rootDir": "./src",
    "outDir": "dist",
    "strict": true,
    "strictNullChecks": false,
    "sourceMap": true,
    "noEmitOnError": false,
    "declaration": true,
    "declarationMap": true,
    "importHelpers": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src"
  ]
}
